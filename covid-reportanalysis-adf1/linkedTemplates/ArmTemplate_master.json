{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-reportanalysis-adf1"
		},
		"covidanalysisdlsa_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'covidanalysisdlsa_ls'"
		},
		"covidanalysissa2134_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'covidanalysissa2134_ls'"
		},
		"HttpServer_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"covidanalysisdlsa_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidanalysisdlsa.dfs.core.windows.net"
		},
		"CopyTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/f2ba2638-9d45-4a9f-8e99-42c1ed0d3954/resourceGroups/Covid-rgrp/providers/Microsoft.Storage/storageAccounts/covidanalysissa2134"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-reportanalysis-adf1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"covidanalysisdlsa_ls_accountKey": {
						"value": "[parameters('covidanalysisdlsa_ls_accountKey')]"
					},
					"covidanalysissa2134_ls_connectionString": {
						"value": "[parameters('covidanalysissa2134_ls_connectionString')]"
					},
					"HttpServer_ls_properties_typeProperties_url": {
						"value": "[parameters('HttpServer_ls_properties_typeProperties_url')]"
					},
					"covidanalysisdlsa_ls_properties_typeProperties_url": {
						"value": "[parameters('covidanalysisdlsa_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covid-reportanalysis-adf1_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CopyTrigger_properties_typeProperties_scope": {
						"value": "[parameters('CopyTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid-reportanalysis-adf1_ArmTemplate_0')]"
			]
		}
	]
}